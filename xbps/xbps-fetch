abi <abi/4.0>,

include <tunables/global>

profile @{bin}/xbps-fetch {
  include <abstractions/base>
  include <abstractions/xbps>
  include <abstractions/nameservice>

  @{bin}/xbps-fetch     mr,
  
  /					r,
  
  /dev/tty				wr,

}

