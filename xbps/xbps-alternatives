abi <abi/4.0>,

include <tunables/global>

profile @{bin}/xbps-alternatives {
  include <abstractions/base>
  include <abstractions/nameservice>

  @{bin}/xbps-alternatives              mr,

  /					r,

  /dev/tty				wr,

  include if exists <local/xbps-alternatives>
}

# vim:syntax=apparmor
